{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:server && yarn build:remix",
    "build:server": "tsup --sourcemap",
    "build:remix": "remix build",
    "dev": "run-p  \"dev:*\"",
    "dev:remix": "remix dev --manual -c  \"nodemon -r ./server/index.ts\"",
    "dev:supabase": "supabase start && supabase status",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "clean": "rm -rf ./build ./public ./.cache ./server/build",
    "gen:types": "ts-node gen-types.ts",
    "typecheck": "tsc"
  },
